{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/shorten",
        "dest": "https://b-mu-two.vercel.app/shorten"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }